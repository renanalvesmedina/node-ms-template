{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "ban-types": false,
        "jsdoc-format": false,
        "no-imports": true,
        "adjacent-overload-signatures": true,
        "member-access": true,
        "no-var-requires": false,
        "prefer-for-of": true,
        "promise-function-async": [true, "check-function-declaration", "check-function-expression", "check-arrow-function", "check-method-declaration"],
        "rules": {"no-empty-interface": true },
        "await-promise": true,
        "no-async-without-await": true,
        "no-duplicate-super": true,
        "no-duplicate-switch-case": true,
        "no-duplicate-variable": true,
        "no-eval": true,
        "no-floating-promises": true,
        "no-invalid-template-strings": true,
        "no-invalid-this": true,
        "no-return-await": true,
        "no-sparse-arrays": true,
        "no-string-literal": false,
        "no-switch-case-fall-through": true,
        "no-tautology-expression": true,
        "no-use-before-declare": true,
        "no-var-keyword": true,
        "switch-default": true,
        "typeof-compare": true,
        "use-isnan": true,
        "deprecation": true,
        "no-duplicate-imports": [true, {"allow-namespace-imports": true}],
        "no-require-imports": false,
        "prefer-const": true,
        "array-type": [true, "array"],
        "arrow-return-shorthand": true,
        "class-name": true,
        "interface-name": [true, "always-prefix"],
        "interface-over-type-literal": true,
        "object-literal-key-quotes": [true, "as-needed"],
        "one-variable-per-declaration": true,
        "ordered-imports": true,
        "prefer-function-over-method": [true, "allow-public", "allow-protected"],
        "space-before-function-paren": [true, "always"],
        "switch-final-break": [true, "always"],
        "align": [true, "parameters", "statements"],
        "eofline": true,
        "import-spacing": true,
        "indent": [true, "tabs", 4],
        "newline-before-return": true,
        "new-parens": true,
        "no-consecutive-blank-lines": false,
        "number-literal-format": true 
    },
    "rulesDirectory": []
}